{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Trace",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Literate"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      { "Name": "DiagnosticsTrace" },
      { "Name": "LiterateConsole" },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\ruinsapi.log"
        }
      }
    ],
    "Properties": {
      "Application": "RuinsApi"
    }
  }
}
