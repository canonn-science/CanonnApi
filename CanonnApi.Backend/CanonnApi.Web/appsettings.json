{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Trace",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Literate"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      { "Name": "DiagnosticsTrace" },
      { "Name": "LiterateConsole" },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\canonnapi_web.log"
        }
      }
    ],
    "Properties": {
      "Application": "CanonnApi.Web"
    }
  },
  "clientSecrets": {
    "clientDomain": "",
    "clientId": "",
    "clientSecret": ""
  },
  "connectionStrings": {
    "ruinsDb": ""
  }
}
