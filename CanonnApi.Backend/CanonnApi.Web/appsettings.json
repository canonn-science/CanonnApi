{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Trace",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Literate"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      { "Name": "DiagnosticsTrace" },
      { "Name": "LiterateConsole" },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\Logs\\canonnapi_web.log"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "Application": "CanonnApi.Web"
    }
  },
  "clientSecrets": {
    "audience": "",
    "clientDomain": "",
    "clientId": "",
    "clientSecret": ""
  },
  "connectionStrings": {
    "canonnDb": ""
  },
  "externalLinks": {
    "edsmSystem": "https://www.edsm.net/en/system/id/{0}/name/{1}",
    "edsmBody": "https://www.edsm.net/en/system/bodies/id/{0}/name/{1}/details/idB/{2}/nameB/{1}+{3}"
  }
}
